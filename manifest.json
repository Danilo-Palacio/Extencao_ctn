{
    "name": "Atalhos CTN",
    "description": "Extensão criada para gerar codigos de arquivos e baixar relatórios de filiados",
    
    "version":"0.0.1",
    "manifest_version":3,
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "windows",
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
      }
}