{
    "name": "Gerador de Códigos - CTN",
    "description": "Extensão criada para gerar codigos de arquivos e baixar relatórios de filiados",

    

    "author": "danilovpalacio@gmail.com",
    "version":"0.0.3",
    "manifest_version":3,

    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
          }
    }
}